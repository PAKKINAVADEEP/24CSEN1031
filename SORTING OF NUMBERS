#include <stdio.h>

int main() {
    int a=6,b=2,c=5,temporary;
    printf("\n Before Sorting:a=%d,b=%d,c=%d",a,b,c);
    if (a>b){//Swap the numbers
    temporary=a;a=b;b=temporary;
    }
    if (a>c){//Swap the numbers
    temporary = a;
    a=c;
    c=temporary;
    }
     if (b>c){//Swap the numbers
    temporary=b;b=c;c=temporary;
    }
    printf("\nAfter Sorting: a=%d,b=%d,c=%d",a,b,c);
    

    return 0;
}
